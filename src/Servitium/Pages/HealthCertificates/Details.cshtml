@page "{id:int}"
@model Servitium.Pages.HealthCertificates.Details

<h1 xmlns="http://www.w3.org/1999/html">Health certificate details</h1>

<form method="get">
    <div class="form-floating mb-3">
        <input asp-for="@Model.Data.TemplateName" class="form-control" readonly/>
        <label asp-for="@Model.Data.TemplateName" class="control-label"></label>
    </div>
    <div class="form-floating mb-3">
        <input asp-for="@Model.Data.Description" class="form-control" readonly/>
        <label asp-for="@Model.Data.Description" class="control-label"></label>
    </div>
    <div class="form-group mt-3">
        <a asp-page="@Model.ReturnUrl" class="btn btn-secondary">Back</a>
    </div>
</form>