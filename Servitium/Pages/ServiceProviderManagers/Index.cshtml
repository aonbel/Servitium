@page
@model Servitium.Pages.ServiceProviderManagers.Index
@{
    ViewData["Title"] = "Service provider managers";
}

<h1 xmlns="http://www.w3.org/1999/html">Service provider managers</h1>

<table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>Service provider name</th>
        <th>Service provider manager name</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var dataInstance in Model.Data)
    {
        <tr>
            <td>@dataInstance.serviceProvider.ShortName</td>
            <td>@dataInstance.person.LastName @dataInstance.person.FirstName</td>
        </tr>
    }
    </tbody>
</table>
